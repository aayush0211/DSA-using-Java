package SortedSingularLL;

public class SortedSingularLL {
	
	public class Node{
		int data;
		Node next;
		public Node(int data) {
			this.data = data;
			next = null;
		}
		
	}
	Node head;
	
	public boolean isEmpty() {
		return head == null;
	}
	
	public void push(int ele) {
		Node newNode = new Node(ele);
		if(isEmpty()) {
			head=newNode;
			return;
		}
	}
}
